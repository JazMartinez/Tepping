extends includes/_layout

block content
    main
        .catalog
            .container
                .catalog-inline
                    aside.sidebar
                        nav.sidebar-menu
                            p.sidebar-menu__title: strong Каталог
                            ul
                                li.sidebar-menu__item: a(href="#")
                                    i.icon-bucket2
                                    span Наливные полы
                                li.sidebar-menu__item: a(href="#")
                                    i.icon-roll-paint
                                    span Краски для бетона
                                li.sidebar-menu__item: a(href="#")
                                    i.icon-playground-2
                                    span Покрытия для детских и&nbsp;спортивных&nbsp;площадок
                                li.sidebar-menu__item.sidebar-menu__item_top: a(href="#")
                                    i.icon-NG
                                    span Teping НГ КМ0
                                li.sidebar-menu__item: a(href="#")
                                    i.icon-hempel
                                    span Hempel
                        .download-file.sidebar__file

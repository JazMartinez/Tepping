extends includes/_layout

block header
    include components/header-product/_header-product
    - var bgPath = "./img/bg_4.jpg";
    - var headerLinks = [{src: '#objects', title: 'Галерея'}, {src: '#materials', title: 'Инструкция'}, {src: '#delivery', title: 'Доставка'}, {src: '#letters', title: 'Отзывы'}];
    - var showBtn = true;
    +productHeader(bgPath, headerLinks, showBtn)

block content
    main#main-content
        .product
            .container
                .product-inline.for-roll
                    .product-image
                        .product-image-block
                            img(src="./img/product-item-4.png" alt="")
                    .product-desc
                        ul.breadcrumbs-line.row
                            li: a(href="#") Главная
                            li.separator: a(href="#") >
                            li: a(href="#") Каталог
                            li.separator: a(href="#") >
                            li: a(href="#") Наливные полы
                            li.separator: a(href="#") >
                            li: a(href="#") Teping Пол 205 C
                        h1.product-title
                            i.icon-title
                            | Roll
                        p Бесшовное покрытие для детских и спортивных площадок. #[br]Для травмобезопасных игр и занятий спортом!
                        ul.product-desc__list
                            li
                                i.icon-quote
                                | Травмобезопасное покрытие
                            li
                                i.icon-quote
                                | Высокая стойкость к ударам и износу
                            li
                                i.icon-quote
                                | Экологически чистое покрытие
                        p.product-price
                            small от
                            span &nbsp;655
                                i.icon-rub
                            | за м#[sup 2]
                        .product-desc-btns
                            a(href="#calculator").btn.btn-black-transparent Рассчитать стоимость
                            a(data-src="#modal-order" href="javascript:;" data-fancybox).btn.btn-red Заказать
                    .product-apply
                        p.apply-title: b Применяется на объектах
                        .apply-inline.row
                            .apply-column.col-md-6.col-xs-12
                                .apply-item
                                    i.icon-gym
                                    p Спортзалы
                                .apply-item
                                    i.icon-playground
                                    p Детские игровые комнаты
                                .apply-item
                                    i.icon-rest-zone
                                    p Зоны отдыха
                                .apply-item
                                    i.icon-skates
                                    p Ледовые катки
        include components/modifications/_modifications
